meta {
  name: Login Success
  type: http
  seq: 2
}

post {
  url: {{BASE_URL}}/admin/login
  body: json
  auth: none
}

body:json {
  {
    "email": "admin@gmail.com",
    "password": "admin"
  }
}

assert {
  res.status: eq 200
  res.body.message: eq OK
  res.body.data.user: isDefined
  res.body.data.token: isString
  res.body.status: isBoolean
  res.body.status: eq true
}

